            </FlowPane>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="5.0" prefHeight="200.0" prefWidth="200.0" vgap="5.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="prihlasitButton" mnemonicParsing="false" onAction="#prihlasitClick" prefHeight="31.0" prefWidth="224.0" text="Přihlásit" />
                  <Button mnemonicParsing="false" onAction="#neregistrovanyClick" prefWidth="100.0" style="-fx-background-color: #aaaaaa;" text="Prohlížení" />
